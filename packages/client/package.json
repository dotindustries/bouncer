  "dependencies": {
    "@dotinc/bouncer-core": "workspace:*"
  },
  "devDependencies": {
    "rimraf": "^4.1.2",
    "ts-toolbelt": "^9.6.0",
    "tsc-alias": "^1.8.2",
    "tslib": "^2.5.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
