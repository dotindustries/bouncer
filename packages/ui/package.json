{
  "name": "@dotinc/bouncer-ui",
  "version": "0.1.0",
  "description": "Bouncer Design System",
  "private": false,
  "keywords": [
    "saas",
    "seat",
    "manager",
    "license",
    "subscription",
    "typescript"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "prebuild": "npm run clean",
    "dev": "tsup --entry src/index.ts --entry src/backend/index.ts --format esm,cjs --dts-resolve --watch",
    "build": "tsup --entry src/index.ts --entry src/backend/index.ts  --format esm,cjs --dts-resolve",
    "clean": "rimraf -r dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotindustries/bouncer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotindustries/bouncer/issues"
  },
  "homepage": "https://github.com/dotindustries/bouncer",
  "dependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@chakra-ui/system": "^2.4.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@saas-ui/react": "^1.8.0",
    "@saas-ui/forms": "^1.5.0",
    "framer-motion": "^7.6.2",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.19.1",
    "react-icons": "^4.7.1"
  },
  "peerDependencies": {
    "next": ">=12"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "^18.0.24",
    "rimraf": "^3.0.2",
    "ts-toolbelt": "^9.6.0",
    "tsc-alias": "^1.7.0",
    "tslib": "^2.4.0",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4",
    "next": "12.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
