{
  "name": "@dotinc/bouncer-admin",
  "version": "0.1.0",
  "description": "Bouncer Admin UI",
  "keywords": [
    "saas",
    "seat",
    "manager",
    "license",
    "subscription",
    "typescript"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./backend": {
      "types": "./dist/backend/index.d.ts",
      "require": "./dist/backend/index.cjs",
      "default": "./dist/backend/index.js"
    }
  },
  "scripts": {
    "prebuild": "npm run clean",
    "dev": "tsup --entry src/index.ts --entry src/backend/index.ts --format esm,cjs --dts --watch",
    "build": "tsup --entry src/index.ts --entry src/backend/index.ts  --format esm,cjs --dts-resolve",
    "clean": "rimraf -r dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotindustries/bouncer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotindustries/bouncer/issues"
  },
  "homepage": "https://github.com/dotindustries/bouncer",
  "dependencies": {
    "@dotinc/bouncer-auth": "workspace:*",
    "@dotinc/bouncer-core": "workspace:*",
    "@dotinc/bouncer-ui": "workspace:*",
    "@tanstack/react-query": "4.22.0",
    "@zodios/core": "10.3.1",
    "@zodios/react": "10.3.3",
    "axios": "1.1.3",
    "react-icons": "^4.7.1",
    "svix-react": "^1.4.2"
  },
  "peerDependencies": {
    "next": ">=12",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "^18.0.24",
    "next": "13.1.4",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^3.0.2",
    "ts-toolbelt": "^9.6.0",
    "tsc-alias": "^1.7.0",
    "tslib": "^2.4.0",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4",
    "zod": "^3.19.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
