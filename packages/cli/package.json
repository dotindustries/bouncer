{
  "name": "@dotinc/bouncer",
  "version": "0.3.1",
  "description": "",
  "private": true,
  "license": "MIT",
  "keywords": [
    "saas",
    "seat",
    "manager",
    "license",
    "subscription",
    "typescript",
    "bouncer",
    "cli"
  ],
  "bin": "dist/cli.cjs",
  "engines": {
    "node": ">=10"
  },
  "type": "module",
  "scripts": {
    "dev:test": "nodemon",
    "build:dev": "npm run clean && tsup src/cli.tsx --format cjs",
    "build": "npm run clean && tsup src/cli.tsx --minify --format cjs",
    "start": "pnpm run build && dist/cli.cjs",
    "pretest": "pnpm run build",
    "test": "xo && ava",
    "clean": "rm -Rf dist",
    "hashbang": "node scripts/hashbang.mjs"
  },
  "files": [
    "dist/cli.cjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotindustries/bouncer.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/dotindustries/bouncer/issues"
  },
  "homepage": "https://github.com/dotindustries/bouncer",
  "dependencies": {
    "@dotinc/bouncer-client": "workspace:*",
    "@dotinc/bouncer-core": "workspace:*",
    "axios": "1.3.4",
    "cli-spinners": "^2.7.0",
    "commander": "^10.0.0",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.3",
    "ink": "^3.2.0",
    "ink-spinner": "^4.0.3",
    "ink-table": "^3.0.0",
    "ink-task-list": "^2.0.0",
    "node-jq": "^2.3.5",
    "pino": "^8.11.0",
    "react": "^18.2.0",
    "update-notifier": "^6.0.2",
    "zod": "^3.20.6",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/ink-divider": "^2.0.2",
    "@types/react": "^18.0.28",
    "@types/update-notifier": "^6.0.2",
    "ava": "^5.2.0",
    "chalk": "^5.2.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink-testing-library": "^2.1.0",
    "nodemon": "2.0.20",
    "pino-pretty": "^9.4.0",
    "rimraf": "^4.1.2",
    "tsc-alias": "^1.8.2",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "xo": "^0.53.1"
  },
  "ava": {
    "typescript": {
      "extensions": [
        "tsx"
      ],
      "rewritePaths": {
        "src/": "dist/"
      }
    }
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "react/prop-types": "off"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
